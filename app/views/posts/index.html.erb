<div class="container">
  <div class="posts">
  <% @posts.each do |post| %>
    <div class="post">
      <h2>
        <%= post.title %>
      </h2>
      <p>
        <%= post.body %>
      </p>
      <div class="actions">
        <%= button_to "Like", root_path, class:"like button" %>
      </div>
    </div>
  <% end %>
  </div>
</div>